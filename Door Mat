# Enter your code here. Read input from STDIN. Print output to STDOUT
n, m = [int(x) for x in input().split()]
i=int((m-3)/2)
j=1
str1="-"
str2=".|."

k=int((m-7)/2)
while (i>=3):
    print("{}{}{}".format(str1*i,str2*j,str1*i))
    i=i-3
    j=j+2
print("{}WELCOME{}".format(str1*k,str1*k))
i=3
j=j-2
while (i<=(m-3)/2):
    print("{}{}{}".format(str1*i,str2*j,str1*i))
    i=i+3
    j=j-2
